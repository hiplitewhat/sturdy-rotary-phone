{
  "version": 2,
  "builds": [
    { "src": "api/M.js", "use": "@vercel/node" },
    { "src": "api/A.js", "use": "@vercel/node" },
    { "src": "api/B.js", "use": "@vercel/node" },
    { "src": "api/A.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/", "dest": "/api/M.js" },
    { "src": "/route2/(.*)", "dest": "/api/A.js" },
    { "src": "/route3/(.*)", "dest": "/api/B.js" },
    { "src": "/route4(/.*)?", "dest": "/api/A.py" }
  ]
}
